int a = 20;

int b = 20;

matrix<float>[a, b] Am;

matrix<float>[b, a] Bm;

matrix<float>[a, a] RESm;

for(int i = 0; i < a; i++){
    for(int j = 0; j < b; j++){
        Am[i,j] = i-j;
    }
}

for(int i = 0; i < a; i++){
    Bm[i,i] = Am[i,i] + 1.0;
}

print("Am matrix");
print(Am);


RESm = Am * Bm;

print("RESm Matrix");
print(RESm);

if((a < b) == true){
    print(a + b);
}