int a = 20;

int b = 20;

matrix<int>[a, b] Am;

matrix<int>[b, a] Bm;

matrix<int>[a, a] RESm;

for(int i = 0; i < a+1; i++){
    for(int j = 0; j < b; j++){
        Am[i,j] = i-j;
    }
}

for(int i = 0; i < a; i++){
    Bm[i,i] = Am[i,i] + 1;
}

print("Am matrix");
print(Am);


RESm = Am * Bm;

print("RESm Matrix");
print(RESm);

if((a < b) == true){
    print(a + b);
}