int64 fibR(int64 n) {
    int64 tmp = 0;
    if (n == 0){
        tmp = 0;
    } else if( n == 1 ){
        tmp = 1;
    } else {
        tmp = fibR(n - 1) + fibR(n - 2);
    }

    return tmp;
}

int64 fibI(int64 n)
{
    int64 tmp1 = 0;
    int64 tmp2 = 1;

    for(int64 i = 0; i < n; i++){
        int64 tmp3 = tmp1;
        tmp1 = tmp2;
        tmp2 += tmp3;
    }

    return tmp1;
}

